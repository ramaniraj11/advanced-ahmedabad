<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_sale_order_ats" model="ir.sequence">
        <field name="name">Sales Order ATS</field>
        <field name="code">sale.order.ats</field>
        <field name="prefix">ATS-Q-%(day)s%(month)s%(year)s-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_sale_order_acr" model="ir.sequence">
        <field name="name">Sales Order ACR</field>
        <field name="code">sale.order.acr</field>
        <field name="prefix">ACR-Q-%(day)s%(month)s%(year)s-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
